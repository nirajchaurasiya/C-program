#include <Windows.h>
1 DIALOG 0, 0, 200, 100
STYLE DS_CENTER | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Custom Dialog"
FONT 10, "Arial"
BEGIN
    CONTROL "OK", IDOK, "Button", BS_DEFPUSHBUTTON | WS_TABSTOP, 10, 60, 80, 24
    CONTROL "Cancel", IDCANCEL, "Button", WS_TABSTOP, 110, 60, 80, 24
    LTEXT "This is a custom dialog box.", -1, 10, 10, 180, 40, SS_CENTER
END
